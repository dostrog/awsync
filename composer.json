{
    "name": "dostrog/awsync",
    "description": "Simple, lightweight console utility to sync local folder with AWS S3 bucket.",
    "keywords": ["utility", "aws s3", "console", "cli"],
    "homepage": "https://github.com/dostrog/awsync",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dostrog/awsync/issues",
        "source": "https://github.com/dostrog/awsync"
    },
    "authors": [
        {
            "name": "Sergey Donin",
            "email": "sergey.donin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "fakerphp/faker": "^1.13",
        "illuminate/translation": "^8.29",
        "laravel-zero/framework": "^8.4",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nunomaduro/laravel-desktop-notifier": "^2.5",
        "padraic/phar-updater": "^1.0.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["awsync"]
}
